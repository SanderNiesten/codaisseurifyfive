
<h1>Codaisseurify</h1>
<hr>
<%= link_to 'New artist', new_artist_path %>
<hr>
<table>
  <tr>
    <th></th>
  </tr>

  <% @artists.each do |artist| %>
    <tr>
      <td><%= link_to artist.name, artist_path(artist)%></td>
      <td><%= link_to 'Edit', edit_artist_path(artist) %></td>
      <td><%= link_to 'Destroy', artist_path(artist),
        method: :delete,
        data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
